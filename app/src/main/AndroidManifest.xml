<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.nus.donedeal">

    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity"></activity>
        <activity android:name=".NamesActivity"></activity>
        <activity android:name=".EditNamesActivity"></activity>
        <activity android:name=".AddExpenditureActivity"></activity>
        <activity android:name=".ViewExpenditureActivity"></activity>
        <activity android:name=".DeleteExpenditureActivity"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>
